#include <stdio.h>
#include <stdlib.h>
#include <locale.h>


int main()
{
    setlocale(LC_ALL, "Portuguese");
    int contador = 0;
    int multi = 0;
    int tab = 0;
    printf("Digite qual tabuada voce quer:  \n");
    scanf("%d", &tab);
    for(contador = 0; contador <= 10; contador++)
       {
            multi = tab * contador;
            printf("%d X %d =  %d\n", tab, contador, multi);
       }
    return 0;
}
