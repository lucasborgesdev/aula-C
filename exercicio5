#include <stdio.h>
#include <stdlib.h>
#include <locale.h>


int main()
{
    setlocale(LC_ALL, "Portuguese");
    int a = 0;
    int b = 0;
    int contador = 1;
    int multi = 0;
    printf("Qual taboada voce quer : \n");
    scanf("%d", &a);
    while(contador <= 11){
            multi = a * b;
            printf("%d x %d = %d\n", a, b, multi);
    b ++;
    contador ++;


    }
}
