#include <stdio.h>
#include <stdlib.h>
#include <locale.h>


int main()
{
    setlocale(LC_ALL, "Portuguese");
   int a = 0;
   int b = 0;
   printf("Digite o primeiro numero que voce quer :\n");
   scanf("%d", &a);
   printf("Digite o ultimo numero que voce quer :\n");
   scanf("%d", &b);
   for(a = a; a <= b; a += 2)
   {
       printf("\n%d ", a);

   }

    return 0;
}
