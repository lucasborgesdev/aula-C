#include <stdio.h>
#include <stdlib.h>
#include <locale.h>


int main()
{
    setlocale(LC_ALL, "Portuguese");
    int contador;
    int impar = 1;


    printf("Digite quantos numeros impares voce quer ver: ");
    scanf("%d", &contador);

    while (contador){
        printf("%d ", impar);
        impar += 2;
        contador--;
    }
    return 0;
}
